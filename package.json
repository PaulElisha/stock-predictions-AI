    "dotenv": "^17.2.3",
    "nodemon": "^3.1.11",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@mistralai/mistralai": "^1.10.0",
    "axios": "^1.13.2",
    "express": "^5.1.0",
    "mongodb": "^6.21.0",
    "mongoose": "^8.19.3"
  }
